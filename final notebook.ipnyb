   "id": "f70e0e38",
   "metadata": {},
   "outputs": [],
   "source": [
    "X.User_Score = X.User_Score.astype(float)\n",
    "X_test.User_Score = X_test.User_Score.astype(float)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 26,
   "id": "1236b25c",
   "metadata": {},
   "outputs": [],
   "source": [
    "#agregation of the cotegorical variable based on frequency\n",
    "for i in X.columns:\n",
    "    if X[i].dtype=='object':\n",
    "        value_dataframe=pd.DataFrame()\n",
    "        value_dataframe[i]=X[i].value_counts()\n",
    "        names_to_approx=value_dataframe[value_dataframe[i]<X.shape[0]*1/100].index\n",
    "        for j in names_to_approx:\n",
    "            X.loc[(X[i]==j), i] ='aggregate_'+i\n",
    "            \n",
    "                    "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 27,
   "id": "d3e6cc7c",
   "metadata": {},
   "outputs": [],
   "source": [
    "#similarly aggregation of variable based on train set\n",
    "\n",
    "for i in X.columns:\n",
    "    if X[i].dtype=='object':\n",
    "        a=set(X[i].unique())\n",
    "        b=set(X_test[i].unique())\n",
    "        c=b.difference(a)\n",
    "        for j in c:\n",
    "            X_test.loc[X_test[i] == j, i] = 'Aggregate_'+i"
   ]
  },
